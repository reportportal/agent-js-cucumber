{
  "name": "reportportal-agent-cucumber",
  "version": "2.0.6",
  "description": "Agent that connects cucumber-js with report portal ",
  "main": "./modules/index",
  "spec": "cucumber version >=3.0.0 <=4.2.1",
  "dependencies": {
    "reportportal-client": "^5.5.0"
  },
  "devDependencies": {
    "chromedriver": "^2.32.2",
    "cucumber-parallel": "^2.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "log4js": "^2.3.3",
    "prettier": "^1.19.1",
    "protractor-cucumber-framework": "^4.0.8",
    "selenium-webdriver": "^3.5.0"
  },
  "peerDependencies": {
    "cucumber": "4.x - 6.x"
